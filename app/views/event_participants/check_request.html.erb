<div class="row">
  <div class="col-md-3">
    <%= link_to @event, :class => "btn btn-default btn-sm" do %>
      <span class="glyphicon glyphicon-chevron-left"></span>&nbsp;<%= t 'cancel' %>
    <% end %>
  </div>
  
  <div class="col-md-6">
    <h4 align="center"><%= "#{t 'add'} #{t 'event_participant.event_participant'} - #{t 'step'} 1 (#{t 'event_participant.check_request'})" %></h4>
  </div>

  <div class="col-md-3" align="right">
    <%= link_to check_all_event_event_participants_path(@event), :class => "btn btn-default btn-sm" do %>
    	<%= "#{t 'go_to'} 2" %>&nbsp;<span class="glyphicon glyphicon-chevron-right"></span>
    <% end %>
  </div>
</div>
<hr>

<div id="passed" class="status_1" align="center">Lolos Pra-syarat Administratif</div>
<div id="check_request" style="height:300px; overflow:auto;"><%= render 'check_request' %></div>

<div id="not_passed" class="status_5" align="center">Tidak Lolos Pra-syarat Administratif</div>
<div id="check_request_not_passed" style="height:300px; overflow:auto;"><%= render 'check_request_not_passed' %></div>

<div id="modal"></div>

<!-- Modal -->
<div id="general_modal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="BranchModalLabel" aria-hidden="true">
  <div class="modal-dialog" style="width:900px;">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
        <h3 class="modal-title"></h3>
      </div>
      <div class="modal-body">
      </div>
    </div>
  </div>
</div>