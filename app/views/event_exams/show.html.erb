<div class="row">
  <div class="col-md-4">
    <%= link_to @event, :class => "btn btn-default btn-sm" do %>
      <span class="glyphicon glyphicon-chevron-left"></span>&nbsp;<%= t 'back' %>
    <% end %>
  </div>
  
  <div class="col-md-4">
    <h4 align="center"><%= t 'event.event' %> - <%= t 'event_exam.event_exam' %> - <%= t 'detail' %></h4>
  </div>

  <div class="col-md-4" align="right">
    <%= link_to export_ans_sheet_event_event_exam_questions_path(@event.id, @event_exam.id, :format => "xls"), :class => "btn btn-info btn-sm" do %>
      <%= "#{t 'export'} #{t 'event_exam.question.answer_sheet'}" %>&nbsp;<span class="glyphicon glyphicon-export"></span>
    <% end %>

    <%= link_to import_ans_sheet_event_event_exam_questions_path(@event.id, @event_exam.id), :class => "btn btn-success btn-sm", :remote => true do %>
      <%= "#{t 'import'} #{t 'event_exam.question.answer_sheet'}" %>&nbsp;<span class="glyphicon glyphicon-import"></span>
    <% end %>
  </div>
</div>

<%= render 'show' %>

<div id="modal"></div>

<!-- Modal -->
<div id="general_modal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="BranchModalLabel" aria-hidden="true">
  <div class="modal-dialog" style="width:85%;">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
        <h3 class="modal-title"></h3>
      </div>
      <div class="modal-body">
      </div>
    </div>
  </div>
</div>